<?xml version="1.0" encoding="utf-8"?>
<resources>

     <style name="Theme.Mobiware" parent="Theme.MaterialComponents.Light.NoActionBar">
          <!-- Primary brand color. -->
          <item name="fontFamily">@font/dosis_medium</item>
          <item name="colorPrimary">@color/cyan</item>
          <item name="colorPrimaryVariant">@color/white</item>
          <item name="colorOnPrimary">@color/cyan</item>
          <!-- Secondary brand color. -->
          <item name="colorSecondary">@color/teal_200</item>
          <item name="colorSecondaryVariant">@color/teal_700</item>
          <item name="colorOnSecondary">@color/black</item>
          <!-- Status bar color. -->
          <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
          <item name="android:textColor">@color/cyan</item>
          <item name="background">?attr/colorPrimaryVariant</item>
          <!-- Customize your theme here. -->
          <item name="colorOnSurface">#fff</item>
          <item name="android:windowSplashScreenBackground">@color/cyan</item>
     </style>

</resources>